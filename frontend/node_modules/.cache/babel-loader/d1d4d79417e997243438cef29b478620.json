{"ast":null,"code":"import { mapState, mapMutations } from \"vuex\";\nexport default {\n  name: 'NavBar',\n  computed: { ...mapState([\"user\"])\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n  },\n\n  unmounted() {\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n\n  methods: { ...mapMutations([\"setUser\"]),\n\n    scrollToTop() {\n      window.scrollTo(0, 0);\n    },\n\n    showNav: function () {\n      let navbar = document.querySelector('.header .navbar');\n      navbar.classList.toggle('active');\n    },\n    showLog: function () {\n      let mq = window.matchMedia(\"(max-width: 768px)\");\n\n      if (mq.matches) {\n        let log = document.querySelector('.drop-down-select');\n        log.classList.toggle('active');\n      }\n    },\n    handleScroll: function () {\n      let navbar = document.querySelector('.header .navbar');\n      navbar.classList.remove('active');\n      let log = document.querySelector('.drop-down-select');\n      log.classList.remove('active');\n    },\n    handleLogout: function () {\n      this.setUser(\"\");\n    }\n  }\n};","map":{"version":3,"mappings":"AA+CA,SAASA,QAAT,EAAmBC,YAAnB,QAAuC,MAAvC;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAGXC,QAAQ,EAAE,EACN,GAAGH,QAAQ,CAAC,CAAC,MAAD,CAAD;EADL,CAHC;;EAOXI,OAAO,GAAG;IACNC,MAAM,CAACC,gBAAP,CAAwB,QAAxB,EAAkC,KAAKC,YAAvC;EACH,CATU;;EAUXC,SAAS,GAAG;IACRH,MAAM,CAACI,mBAAP,CAA2B,QAA3B,EAAqC,KAAKF,YAA1C;EACH,CAZU;;EAcXG,OAAO,EAAE,EACL,GAAGT,YAAY,CAAC,CAAC,SAAD,CAAD,CADV;;IAGLU,WAAW,GAAG;MACVN,MAAM,CAACO,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;IACH,CALI;;IAOLC,OAAO,EAAE,YAAY;MACjB,IAAIC,MAAK,GAAIC,QAAQ,CAACC,aAAT,CAAuB,iBAAvB,CAAb;MACAF,MAAM,CAACG,SAAP,CAAiBC,MAAjB,CAAwB,QAAxB;IACH,CAVI;IAYLC,OAAO,EAAE,YAAY;MACjB,IAAIC,EAAC,GAAIf,MAAM,CAACgB,UAAP,CAAkB,oBAAlB,CAAT;;MACA,IAAID,EAAE,CAACE,OAAP,EAAgB;QACZ,IAAIC,GAAE,GAAIR,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,CAAV;QACAO,GAAG,CAACN,SAAJ,CAAcC,MAAd,CAAqB,QAArB;MACJ;IACH,CAlBI;IAoBLX,YAAY,EAAE,YAAY;MACtB,IAAIO,MAAK,GAAIC,