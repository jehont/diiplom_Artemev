ass=\"admin-form-container\">\n            <form id=\"adminForm\" @submit=\"handleSubmit\" novalidate autocomplete=\"off\">\n                <h3>Администратор</h3>\n\n                <div v-if=\"errors.length\" class=\"error-box\">\n                    <ul>\n                        <li v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n                    </ul>\n                </div>\n\n                <div class=\"form-group\">\n                    <input type=\"password\" id=\"uPass\" name=\"uPass\" class=\"form-control\"\n                        placeholder=\"Введите пароль администратора\" v-model=\"adminObj.pass\" />\n                </div>\n\n                <div class=\"form-group\">\n                    <input type=\"submit\" value=\"доступ к панели администратора\" class=\"btn\">\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n\n<script>\nimport { mapMutations } from \"vuex\";\nexport default {\n    name: 'Admin',\n\n    data() {\n        return {\n            adminObj: { pass: \"\" },\n            key: \"25082002\",\n            errors: [],\n        }\n    },\n\n    methods: {\n        ...mapMutations([\"setAdmin\"]),\n\n        handleSubmit(e) {\n            this.errors = [];\n            if (!this.adminObj.pass) {\n                this.errors.push('Необходим пароль');\n            }\n\n            if (!this.errors.length == 0) {\n                e.preventDefault();\n            }\n            else {\n                e.preventDefault();\n                if (this.key === this.adminObj.pass) {\n                    this.setAdmin(\"admin\");\n                    this.$router.push(\"/admin/dashboard\");\n                }\n                else {\n                    this.errors.push(\"Пароль неверный!\")\n                }\n\n            }\n        }\n    }\n}\n</script>\n\n<style scoped>\n.admin-container {\n    padding: 2rem 9%;\n}\n\n.admin-container .admin-form-container {\n    background-color: #fff;\n    