{"ast":null,"code":"import { mapMutations } from \"vuex\";\nexport default {\n  name: 'Admin',\n\n  data() {\n    return {\n      adminObj: {\n        pass: \"\"\n      },\n      key: \"25082002\",\n      errors: []\n    };\n  },\n\n  methods: { ...mapMutations([\"setAdmin\"]),\n\n    handleSubmit(e) {\n      this.errors = [];\n\n      if (!this.adminObj.pass) {\n        this.errors.push('Необходим пароль');\n      }\n\n      if (!this.errors.length == 0) {\n        e.preventDefault();\n      } else {\n        e.preventDefault();\n\n        if (this.key === this.adminObj.pass) {\n          this.setAdmin(\"admin\");\n          this.$router.push(\"/admin/dashboard\");\n        } else {\n          this.errors.push(\"Пароль неверный!\");\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2BA,SAASA,YAAT,QAA6B,MAA7B;AACA,eAAe;EACXC,IAAI,EAAE,OADK;;EAGXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QAAEC,IAAI,EAAE;MAAR,CADP;MAEHC,GAAG,EAAE,UAFF;MAGHC,MAAM,EAAE;IAHL,CAAP;EAKH,CATU;;EAWXC,OAAO,EAAE,EACL,GAAGP,YAAY,CAAC,CAAC,UAAD,CAAD,CADV;;IAGLQ,YAAY,CAACC,CAAD,EAAI;MACZ,KAAKH,MAAL,GAAc,EAAd;;MACA,IAAI,CAAC,KAAKH,QAAL,CAAcC,IAAnB,EAAyB;QACrB,KAAKE,MAAL,CAAYI,IAAZ,CAAiB,kBAAjB;MACJ;;MAEA,IAAI,CAAC,KAAKJ,MAAL,CAAYK,MAAb,IAAuB,CAA3B,EAA8B;QAC1BF,CAAC,CAACG,cAAF;MACJ,CAFA,MAGK;QACDH,CAAC,CAACG,cAAF;;QACA,IAAI,KAAKP,GAAL,KAAa,KAAKF,QAAL,CAAcC,IAA/B,EAAqC;UACjC,KAAKS,QAAL,CAAc,OAAd;UACA,KAAKC,OAAL,CAAaJ,IAAb,CAAkB,kBAAlB;QACJ,CAHA,MAIK;UACD,KAAKJ,MAAL,CAAYI,IAAZ,CAAiB,kBAAjB;QACJ;MAEJ;IACJ;;EAvBK;AAXE,CAAf","names":["mapMutations","name","data","adminObj","pass","key","errors","methods","handleSubmit","e","push","length","preventDefault","setAdmin","$router"],"sourceRoot":"","sources":["D:\\diiplom_Artemev\\frontend\\src\\admin\\Admin.vue"],"sourcesContent":["<template>\n    <div class=\"admin-container\">\n        <div class=\"admin-form-container\">\n            <form id=\"adminForm\" @submit=\"handleSubmit\" novalidate autocomplete=\"off\">\n                <h3>Адми